---
import {sections} from '../helpers/const.js'
---

<section class="w-full min-h-auto-[480px] flex flex-col lg:flex-row justify-center items-center gap-10 p-4">
  {sections.map(section => {
    return (
    <article class="w-[480px] h-96 border relative">
      <img src={section.img} />
    </article>
    )
  })}

</section>


