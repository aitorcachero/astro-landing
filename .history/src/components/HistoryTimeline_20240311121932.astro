---
import VerticalTimeline from './VerticalTimeline.astro';
import './timeline_styles.css';
import { HISTORY } from '../helpers/const.js';
import VerticalTimelineElement from './VerticalTimelineElement.astro';
---

<VerticalTimeline>
  {
    HISTORY.map((element, i) => {
      return <VerticalTimelineElement element={element} i={i} />;
    })
  }
</VerticalTimeline>
