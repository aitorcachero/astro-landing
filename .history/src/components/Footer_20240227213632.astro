---
import { sections, partners, footer } from '../helpers/const.js'
---

<footer class="h-96 flex flex-col justify-center items-center gap-10" style={{backgroundColor: '#2E5171'}}>
  <section class="flex flex-row gap-10">
    {partners.map(partner => {
      return (
        <a href={partner.url}>
          <img src={partner.img} class="h-20"/>
        </a>
      )
    })}
  </section>
  <section class="flex flex-row gap-10">
    {sections.map(section => {
      return (
        <a href=''>
          <h2 class="text-white font-medium">{section.name}</h2>
        </a>
      )
    })}
  </section>
</footer>


