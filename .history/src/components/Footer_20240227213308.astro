---
import { sections, partners, footer } from '../helpers/const.js'
---

<footer class="h-96 flex flex-col justify-center items-center" style={{backgroundColor: '#2E5171'}}>
  <section class="flex flex-row">
    {partners.map(partner => {
      return (
        <a>
          <img src={partner.img} class="w-48"/>
        </a>
      )
    })}
  </section>
</footer>


